{
  "name": "research",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.9",
    "chokidar": "^4.0.1",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "fast-json-stable-stringify": "^2.1.0",
    "netlify": "^13.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-iframe": "^1.8.5",
    "react-markdown": "^9.0.3",
    "react-router-dom": "^6.26.2",
    "source-map": "^0.7.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev:research": "VITE_APP=ux-research vite --host",
    "dev:metrics": "VITE_APP=ux-metrics vite --host",
    "dev:visions": "VITE_APP=ux-metrics vite --host",
    "dev:win": "VITE_APP=ux-metrics vite --host",
    "build": "vite build",
    "build:research": "VITE_APP=ux-research vite build && cp ./netlify.toml ./ux-research/dist/netlify.toml && cp -R ./ux-research/dist/ ./docs/",
    "build:metrics": "VITE_APP=ux-metrics vite build  && cp ./netlify.toml ./ux-metrics/dist/netlify.toml && cp -R ./ux-metrics/dist/ ./docs/",
    "build:visions": "VITE_APP=ux-visions vite build  && cp ./netlify.toml ./ux-visions/dist/netlify.toml && cp -R ./ux-visions/dist/ ./docs/",
    "build:win": "VITE_APP=win-stakeholders vite build  && cp ./netlify.toml ./win-stakeholders/dist/netlify.toml && cp -R ./win-stakeholders/dist/ ./docs/",
    "build:all": "npm run build:research && npm run build:metrics && npm run build:visions && npm run build:win",
    "deploy:research": "npm run build:research && netlify deploy --dir ./ux-research/dist/ --site ae39bcfa-e070-4429-8611-db3ae4462696",
    "deploy:metrics": "npm run build:research && netlify deploy --dir ./ux-metrics/dist/ --site d0113b5c-34df-4283-ac63-de67f2d7f715 ",
    "preview": "vite preview"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "netlify-cli": "^19.0.2",
    "nodemon": "^3.1.7",
    "vite": "^6.1.1"
  }
}